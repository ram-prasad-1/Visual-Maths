<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.visualfiber.apps.visualmaths">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ac1.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ac2.ContentTypeActivity" />
        <activity android:name=".ac3_list_problem_set.ProblemListActivity" />
        <activity android:name=".ac3_list_tools.ToolListActivity" />
        <activity android:name=".ac4_display_problem_set.DisplayQuestionActivity" />
        <activity android:name=".ac4_display_tools.DisplayToolsActivity"></activity>
    </application>

</manifest>